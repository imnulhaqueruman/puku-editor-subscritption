{
  "name": "puku-subscription-service",
  "version": "1.0.0",
  "description": "OpenRouter API key subscription service with credit-based system",
  "main": "src/index.ts",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "deploy:staging": "wrangler deploy --env staging",
    "tail": "wrangler tail",
    "db:create": "wrangler d1 create puku-subscription-db",
    "db:migrate": "wrangler d1 execute puku-subscription-db --file=./schema.sql",
    "db:migrate:local": "wrangler d1 execute puku-subscription-db --local --file=./schema.sql",
    "db:query": "wrangler d1 execute puku-subscription-db --command",
    "test": "vitest"
  },
  "keywords": ["cloudflare-workers", "openrouter", "api-key-management"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241127.0",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.3",
    "wrangler": "^3.80.0",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "hono": "^4.0.0",
    "jose": "^5.2.0"
  }
}
